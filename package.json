{
    "name": "adventofcode",
    "version": "1.0.0",
    "license": "MIT",
    "devDependencies": {
        "eslint": "6.8.0",
        "eslint-config-google": "0.14.0",
        "eslint-plugin-import": "2.20.0",
        "husky": "4.2.1",
        "lint-staged": "10.0.1",
        "prettier": "1.19.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
